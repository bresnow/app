  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"tailwindcss -i ./src/styles/input.css -o ./public/style.css --watch\" \"nodemon server.mjs -w src/**/*\" \"vite --host\"",
    "deploy": "NAME=fltngmmth VOLUME=$PWD docker stack deploy -c docker-stack.yml fltngmmth",
