    "esbuild": "^0.17.11",
    "express": "^4.18.2",
    "nodemon": "^2.0.21",
    "postcss": "^8.3.11",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.17",
    "tailwindcss-pseudo-elements": "^2.0.0",
    "vhtml": "^2.2.0",
    "vite": "^3.0.0",
    "vite-plugin-inspect": "^0.7.15",
    "vite-plugin-node": "^3.0.2"
  },
  "dependencies": {
    "Buffer": "0.0.0",
    "alpinejs": "^3.12.0",
    "css-to-tailwindcss": "^1.0.4",
    "gun": "^0.2020.1239",
    "html2canvas": "^1.4.1",
    "ip": "^1.1.8",
    "joy-jsx": "1.0.20",
    "jquery": "^3.6.4",
